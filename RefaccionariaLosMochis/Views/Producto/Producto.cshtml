    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header bg-dark text-white">
                <h5 class="modal-title" id="exampleModalLabel">Imagenes</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div id="galeriaIMG">
                    <div id="mostrarGrande">
                        <div id="SeleccionarImagen" class="w-auto rounded-3 d-flex justify-content-center align-items-center">
                            <span class="h3 text-white">
                                Seleccione una imagen
                                <i class="fas fa-images fa-1x m-1"></i>
                            </span>
                        </div>

                    </div>
                </div>
                <div id="contenedor-imagenes" class="row">

                </div>
                <div class="w-auto ">
                    <button id="btn-borrarIMG" class="btn btn-sm btn-danger">
                        <i class="fas fa-trash"></i>
                        Borrar IMG
                    </button>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>


<!-- Botón para abrir el modal -->
<button type="button" id="BtnExportarExcel" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#campoModal">
    Seleccionar Campos
</button>

<!-- Modal -->
<div class="modal fade" id="campoModal" tabindex="-1" aria-labelledby="campoModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="campoModalLabel">Selecciona los campos</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form id="campoForm">
                    <div class="row">
                        <div class="col-md-6" id="ColumnasTablaProductos">

                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="P.(NOMBRE DE LA COL)" id="(NOMBRE DE LA COL)" >
                                <label class="form-check-label" for="(NOMBRE DE LA COL)">(NOMBRE DE LA COL)</label>
                            </div>

                        </div>
                        <div class="col-md-6">

                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="M.Descripcion" id="IdMarca">
                                <label class="form-check-label" for="IdMarca">Marca</label>
